<!--pages/result/result.wxml-->
<view class='parent'>
  <view class='con'>
    <view class='point'>
      <text class='point_txt'>{{right}}</text>
      <text class='point_sub'>分</text>
    </view>
    <text class='tip'>{{tip}}</text>
    <view class='btn'>
      <text class='wrong' bindtap='toWrong'>查看错题</text>
      <text class='wrong' bindtap='toTest'>继续测试</text>

    </view>
  </view>




</view>